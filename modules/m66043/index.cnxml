<m:mtext>Convert 30% to a decimal.</m:mtext>
</m:mtd>
</m:mtr>

<m:mtr>
<m:mtd>
<m:mi>P</m:mi>
</m:mtd>
<m:mtd>
<m:mo>=</m:mo>
</m:mtd>
<m:mtd>
<m:mn>.30</m:mn>
</m:mtd>
