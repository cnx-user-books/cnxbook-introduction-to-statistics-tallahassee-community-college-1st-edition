<m:mn>22</m:mn>
<m:mo>,</m:mo>
<m:mfrac>
<m:mn>8</m:mn>
<m:mrow>
<m:msqrt>
<m:mn>80</m:mn>
</m:msqrt>
</m:mrow>
</m:mfrac>
